# Engy-5310 Problem 1: Poisson 1D FEM Neutron Diffusion Equation
# UMass Lowell Nuclear Chemical Engineering
# Paul Johnson

# Parameters
xmin = 0.00000e+00
xmax = 1.00000e+01
diff_coeff = 0.87000e+00
u_left = 1.000e+07
velocity = 2.200E+03
transfer_coeff_right = velocity/2
sigma_a = 0.022
sigma_s12 = 0.007
sigma_s13 = 0.0
sigma_s23 = 0.0

[Problem]
  type = FEProblem
  coord_type = RSPHERICAL
[]

[Mesh]
  [1d]
    type = GeneratedMeshGenerator
    dim = 1
    xmin = ${replace xmin}
    xmax = ${replace xmax}
    nx = 10000
    elem_type = edge3
  []
[]

[Variables]
  [u1]
    order = second
    family = lagrange
    initial_condition = ${replace u_left}
  []
  [u2]
    order = second
    family = lagrange
    initial_condition = ${replace u_left}
  []
 
[]

[Kernels]
  [diffusion-term-1]
    type = DiffusionTerm
    variable = u1     # produced quantity
    diffCoeff = ${replace diff_coeff}
  []
  [diffusion-term-2]
    type = DiffusionTerm
    variable = u2     # produced quantity
    diffCoeff = ${replace diff_coeff}
  []
  
  [absorption-term-1]
    type = AbsorptionTerm
    variable = u1     # produced quantity
    sigmaA = ${replace sigma_a}
  []
  [absorption-term-2]
    type = AbsorptionTerm
    variable = u2     # produced quantity
    sigmaA =  ${replace sigma_s12}
  []

 #[source-term-1]
  #  type = SourceTerm
    #variable = u1
    #coupledGroupA = u1
    #coupledGroupB = u1
  #[]
  [source-term-2]
    type = SourceTerm
    variable = u2
    coupledGroupA = u1
    sigma_sa =${replace sigma_s12}
    coupledGroupB = u1
  []

[]

[BCs]
  [entry-u1]
    type = DirichletBC
    variable = u1
    boundary = left
    value = ${replace u_left}
  []
  [exit-u1]
    type = VacuumBC
    variable = u1
    boundary = right
  []
  [entry-u2]
    type = NeumannBC
    variable = u2
    boundary = left
    value = 0
  []
  [exit-u2]
    type = VacuumBC
    variable = u2
    boundary = right
     
  []

[]

[Preconditioning]
  active = 'fdp-newt-full'
  [fdp-newt-full]
    type = FDP
    full = true
    solve_type = 'NEWTON'
    petsc_options_iname = '-pc_type -mat_fd_coloring_err -mat_fd_type'
    petsc_options_value = 'lu  9.9999999999999995474811182588626e-07               ds'
  []
[]

[Executioner]
  type = Steady
[]

[VectorPostprocessors]
  [x-data]
    type = LineValueSampler
    execute_on = 'timestep_end final'
    variable = 'u1 u2'    # output data
    start_point = '${replace xmin} 0 0'
    end_point = '${replace xmax} 0 0'
    num_points = 6001
    sort_by = id
  []
[]

[Outputs]
  console = true
  [file-x-data]
    type = CSV
    file_base = 'output1G'
    execute_on = 'final'
    show = 'x-data'
  []
[]
